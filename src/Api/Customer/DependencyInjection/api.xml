        </service>
        <service class="Shopware\Api\Customer\Repository\CustomerRepository" id="Shopware\Api\Customer\Repository\CustomerRepository" public="true">
          <argument id="Shopware\Api\Entity\Dbal\EntityReader" type="service"/>
          <argument id="Shopware\Api\Entity\Write\EntityWriter" type="service"/>
          <argument id="Shopware\Api\Entity\Dbal\EntitySearcher" type="service"/>
          <argument id="Shopware\Api\Entity\Dbal\EntityAggregator" type="service"/>
          <argument id="event_dispatcher" type="service"/>
        </service>
         <service class="Shopware\Api\Customer\Definition\CustomerAddressDefinition" id="Shopware\Api\Customer\Definition\CustomerAddressDefinition" >
            <tag name="shopware.entity.definition" entity="customer_address" />
        </service>
        <service class="Shopware\Api\Customer\Repository\CustomerAddressRepository" id="Shopware\Api\Customer\Repository\CustomerAddressRepository" public="true">
          <argument id="Shopware\Api\Entity\Dbal\EntityReader" type="service"/>
          <argument id="Shopware\Api\Entity\Write\EntityWriter" type="service"/>
          <argument id="Shopware\Api\Entity\Dbal\EntitySearcher" type="service"/>
          <argument id="Shopware\Api\Entity\Dbal\EntityAggregator" type="service"/>
          <argument id="event_dispatcher" type="service"/>
        </service>
         <service class="Shopware\Api\Customer\Definition\CustomerGroupDefinition" id="Shopware\Api\Customer\Definition\CustomerGroupDefinition" >
            <tag name="shopware.entity.definition" entity="customer_group" />
        </service>
        <service class="Shopware\Api\Customer\Repository\CustomerGroupRepository" id="Shopware\Api\Customer\Repository\CustomerGroupRepository" public="true">
          <argument id="Shopware\Api\Entity\Dbal\EntityReader" type="service"/>
          <argument id="Shopware\Api\Entity\Write\EntityWriter" type="service"/>
          <argument id="Shopware\Api\Entity\Dbal\EntitySearcher" type="service"/>
          <argument id="Shopware\Api\Entity\Dbal\EntityAggregator" type="service"/>
          <argument id="event_dispatcher" type="service"/>
        </service>
         <service class="Shopware\Api\Customer\Definition\CustomerGroupDiscountDefinition" id="Shopware\Api\Customer\Definition\CustomerGroupDiscountDefinition" >
            <tag name="shopware.entity.definition" entity="customer_group_discount" />
        </service>
        <service class="Shopware\Api\Customer\Repository\CustomerGroupDiscountRepository" id="Shopware\Api\Customer\Repository\CustomerGroupDiscountRepository" public="true">
          <argument id="Shopware\Api\Entity\Dbal\EntityReader" type="service"/>
          <argument id="Shopware\Api\Entity\Write\EntityWriter" type="service"/>
          <argument id="Shopware\Api\Entity\Dbal\EntitySearcher" type="service"/>
          <argument id="Shopware\Api\Entity\Dbal\EntityAggregator" type="service"/>
          <argument id="event_dispatcher" type="service"/>
        </service>
         <service class="Shopware\Api\Customer\Definition\CustomerGroupTranslationDefinition" id="Shopware\Api\Customer\Definition\CustomerGroupTranslationDefinition" >
            <tag name="shopware.entity.definition" entity="customer_group_translation" />
        </service>
        <service class="Shopware\Api\Customer\Repository\CustomerGroupTranslationRepository" id="Shopware\Api\Customer\Repository\CustomerGroupTranslationRepository" public="true">
          <argument id="Shopware\Api\Entity\Dbal\EntityReader" type="service"/>
          <argument id="Shopware\Api\Entity\Write\EntityWriter" type="service"/>
          <argument id="Shopware\Api\Entity\Dbal\EntitySearcher" type="service"/>
          <argument id="Shopware\Api\Entity\Dbal\EntityAggregator" type="service"/>
          <argument id="event_dispatcher" type="service"/>
        </service>
    </services>
</container>                
                