<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
         <service class="Shopware\Api\Version\Definition\VersionDefinition" id="shopware.version.version_definition" >
            <tag name="shopware.entity.definition" entity="version" />
        </service>
        <service class="Shopware\Api\Version\Repository\VersionRepository" id="Shopware\Api\Version\Repository\VersionRepository" public="true">
          <argument id="shopware.api.entity_reader" type="service"/>
          <argument id="shopware.api.entity_writer" type="service"/>
          <argument id="shopware.api.entity_searcher" type="service"/>
          <argument id="shopware.api.entity_aggregator" type="service"/>
          <argument id="event_dispatcher" type="service"/>
        </service>
        <service class="Shopware\Api\Version\Definition\VersionCommitDefinition" id="Shopware\Api\Version\Definition\VersionCommitDefinition" >
            <tag name="shopware.entity.definition" entity="version_change" />
        </service>
        <service class="Shopware\Api\Version\Repository\VersionCommitRepository" id="Shopware\Api\Version\Repository\VersionCommitRepository" public="true">
            <argument id="shopware.api.entity_reader" type="service"/>
            <argument id="shopware.api.entity_writer" type="service"/>
            <argument id="shopware.api.entity_searcher" type="service"/>
            <argument id="shopware.api.entity_aggregator" type="service"/>
            <argument id="event_dispatcher" type="service"/>
        </service>
        <service class="Shopware\Api\Version\Definition\VersionCommitDataDefinition" id="Shopware\Api\Version\Definition\VersionCommitDataDefinition" >
            <tag name="shopware.entity.definition" entity="version_change" />
        </service>
        <service class="Shopware\Api\Version\Repository\VersionCommitDataRepository" id="Shopware\Api\Version\Repository\VersionCommitDataRepository" public="true">
            <argument id="shopware.api.entity_reader" type="service"/>
            <argument id="shopware.api.entity_writer" type="service"/>
            <argument id="shopware.api.entity_searcher" type="service"/>
            <argument id="shopware.api.entity_aggregator" type="service"/>
            <argument id="event_dispatcher" type="service"/>
        </service>
    </services>
</container>                
                