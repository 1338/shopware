<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
         <service class="Shopware\Api\Audit\Definition\AuditLogDefinition" id="Shopware\Api\Audit\Definition\AuditLogDefinition" >
            <tag name="shopware.entity.definition" entity="audit_log" />
        </service>
        <service class="Shopware\Api\Audit\Repository\AuditLogRepository" id="Shopware\Api\Audit\Repository\AuditLogRepository" public="true">
          <argument id="Shopware\Api\Entity\Dbal\EntityReader" type="service"/>
          <argument id="Shopware\Api\Entity\Write\EntityWriter" type="service"/>
          <argument id="Shopware\Api\Entity\Dbal\EntitySearcher" type="service"/>
          <argument id="Shopware\Api\Entity\Dbal\EntityAggregator" type="service"/>
          <argument id="event_dispatcher" type="service"/>
        </service>
    </services>
</container>                
                