{
  "name": "shopware/shopware",
  "description": "Shopware is the next generation of open source e-commerce software made in Germany",
  "keywords": ["shopware", "shop"],
  "homepage": "http://www.shopware.com",
  "type": "project",
  "license": [
    "AGPL-3.0",
    "proprietary"
  ],
  "autoload": {
    "psr-4": {
      "Shopware\\": "src/",
      "": [
        "custom/plugins/",
        "custom/themes/"
      ]
    },
    "classmap": [
      "app/AppKernel.php"
    ],
    "exclude-from-classmap": [
      "custom/plugins/",
      "custom/themes/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.1.0",
    "symfony/symfony": "~3.4",
    "symfony/swiftmailer-bundle": "^3.1",
    "symfony/monolog-bundle": "^3.1",
    "symfony/assetic-bundle": "^2.8",
    "symfony/dotenv": "~3.4",
    "doctrine/doctrine-bundle": "^1.8",
    "doctrine/doctrine-cache-bundle": "^1.3",
    "sensio/distribution-bundle": "^5.0",
    "sensio/framework-extra-bundle": "^5.1",
    "twig/twig": "^2.4",
    "twig/extensions": "^1.5",
    "oyejorge/less.php": "1.7.0.14",
    "linkorb/jsmin-php": "1.0.0",
    "egulias/email-validator": "~2.1",
    "cocur/slugify": "^3.0",
    "superbalist/flysystem-google-storage": "^5.0",
    "league/flysystem": "^1.0",
    "league/flysystem-aws-s3-v3": "^1.0",
    "league/flysystem-azure": "^1.0",
    "ramsey/uuid": "^3.7",
    "voku/stop-words": "^2.0",
    "lexik/jwt-authentication-bundle": "^2.4"
  },
  "require-dev": {
    "symfony/phpunit-bridge": "~3.4",
    "fzaninotto/faker": "^1.7",
    "phpunit/phpunit": "~6.5",
    "mbezhanov/faker-provider-collection": "^1.0"
  },
  "scripts": {
    "symfony-scripts": [
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets"
    ],
    "post-install-cmd": [
      "@symfony-scripts"
    ],
    "post-update-cmd": [
      "@symfony-scripts"
    ]
  },
  "extra": {
    "symfony-app-dir": "app",
    "symfony-bin-dir": "bin",
    "symfony-var-dir": "var",
    "symfony-web-dir": "web",
    "symfony-assets-install": "relative",
    "branch-alias": null
  }
}
